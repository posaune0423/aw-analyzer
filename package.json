{
  "name": "aw-analyzer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "aw-analyzer": "src/main.ts"
  },
  "scripts": {
    "format": "prettier --check . --experimental-ternaries --experimental-cli",
    "format:fix": "prettier --write . --experimental-ternaries --experimental-cli",
    "typecheck": "tsgo --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "bun src/main.ts tick",
    "test": "bun test tests",
    "tick": "bun src/main.ts tick",
    "install-service": "bun src/main.ts install",
    "uninstall-service": "bun src/main.ts uninstall",
    "reset": "bun src/main.ts reset",
    "help": "bun src/main.ts help",
    "version": "bun src/main.ts version",
    "metrics": "bun scripts/oneshot.ts metrics",
    "summary": "bun scripts/oneshot.ts summary",
    "report": "bun scripts/oneshot.ts report",
    "alert": "bun scripts/oneshot.ts alert",
    "weekly-report": "bun src/main.ts weekly-report",
    "knip": "knip"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@prettier/plugin-oxc": "^0.1.3",
    "@types/bun": "latest",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "knip": "^5.79.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript-eslint": "^8.51.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "prettier": {
    "printWidth": 120,
    "arrowParens": "avoid",
    "experimentalTernaries": true,
    "plugins": [
      "prettier-plugin-organize-imports",
      "@prettier/plugin-oxc",
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@slack/webhook": "^7.0.6",
    "@t3-oss/env-core": "^0.13.10",
    "neverthrow": "^8.2.0",
    "openai": "^6.15.0",
    "zod": "^4.3.4"
  }
}
